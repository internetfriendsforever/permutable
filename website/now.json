{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@now/static" },
    { "src": "../library/dist/*.js", "use": "@now/static" }
  ],

  "routes": [
    { "src": "/examples/(.*)", "dest": "/examples/$1"},
    { "src": "/dist/(.*)", "dest": "../library/dist/$1"},
    { "src": "/", "dest": "/index.html"}
  ]
}
