{
  "version": 2,
  "name": "permutable",
  "alias": "permutable.xyz",
  "builds": [
    { "src": "index.html", "use": "@now/static" },
    { "src": "examples/**/*", "use": "@now/static" },
    { "src": "dist/*.js", "use": "@now/static" }
  ],

  "routes": [
    { "src": "/examples/(.*)", "dest": "/examples/$1"},
    { "src": "/dist/(.*)", "dest": "/dist/$1"},
    { "src": "/", "dest": "/index.html"}
  ]
}
